{
  "buildCommand": "prisma generate && next build",
  "installCommand": "npm install",
  "framework": null,
  "env": {
    "SKIP_ENV_VALIDATION": "1"
  },
  "functions": {
    "pages/api/**/*.js": {
      "maxDuration": 30
    },
    "src/app/api/**/*.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}