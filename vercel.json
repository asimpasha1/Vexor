{
  "buildCommand": "node scripts/vercel-build.js",
  "installCommand": "npm ci",
  "framework": null,
  "outputDirectory": ".next",
  "env": {
    "SKIP_ENV_VALIDATION": "1",
    "NEXT_TELEMETRY_DISABLED": "1",
    "NODE_ENV": "production"
  },
  "functions": {
    "src/app/api/**/*.js": {
      "maxDuration": 30,
      "runtime": "nodejs18.x"
    }
  }
}